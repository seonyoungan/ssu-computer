package chap04.com.dao;

public class Person {
	private int id=2018101004;
	private String name="홍길순";
	
	public Person() {
		
	}
	
	public int getId() {
		return id;
	}
	
	public void setID(int id) {
		this.id=id;
	}
	
	public String getName() {
		return name;
	}
	
	
	public void setName(String name) {
		this.name=name;
	}
}
